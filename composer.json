{
    "name": "api-skeletons/laravel-doctrine-data-fixtures",
    "description": "Doctrine Data Fixtures for Laravel",
    "type": "library",
    "require": {
        "php": "^8.0",
        "doctrine/data-fixtures": "^1.5",
        "laravel/framework": "^8.82 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "doctrine/coding-standard": "^9",
        "doctrine/dbal": "^2.13",
        "laravel-doctrine/orm": "^1.7",
        "orchestra/testbench": "^6.24",
        "vimeo/psalm": "^4.20 || ^5.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApiSkeletons\\Laravel\\Doctrine\\DataFixtures\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiSkeletonsTest\\Laravel\\Doctrine\\DataFixtures\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ApiSkeletons\\Laravel\\Doctrine\\DataFixtures\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
